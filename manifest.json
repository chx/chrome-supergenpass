{
	"name": "ChromeGenPass",
	"version": "1.0.1",

	"description": "Comfortable use of SuperGenPass from Google Chrome.",
	"icons": {
		"16": "img/icon16.png",
		"48": "img/icon48.png",
		"128": "img/icon128.png"
	},

	"background_page": "html/background.html",
	"options_page": "options/options.html",
	"content_scripts": [
		{
			"matches": ["http://*/*", "https://*/*"],
			"js": ["js/contentscript.js"]
		}
	],
	"permissions": [
		"tabs", "http://*/*", "https://*/*"
	]
}